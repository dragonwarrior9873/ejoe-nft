{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/create-nft/page.jsx",
    "src/hooks/useContract.ts",
    "src/hooks/useFetchAllNFTs.js",
    "model/userModel.js",
    "src/hooks/useFetchEventForNFT.js",
    "src/app/author/following/[id]/page.jsx",
    "src/app/author/followers/[id]/page.jsx",
    "src/app/author/created/[id]/page.jsx",
    "src/app/author/[id]/page.jsx"
  ],
  "exclude": ["node_modules"]
}
